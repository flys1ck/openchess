{
  "name": "openchess",
  "private": true,
  "version": "0.0.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devtools": "vue-devtools",
    "build": "vue-tsc --noEmit && vite build",
    "tauri": "tauri",
    "db:gen:types": "kysely-codegen --dialect sqlite --exclude-pattern __* --out-file src/database.d.ts",
    "format": "prettier --ignore-path .prettierignore --ignore-path .gitignore --write .",
    "lint": "eslint . --ext .vue,.js,.cjs,.ts --fix --ignore-path .prettierignore",
    "lint:types": "vue-tsc --noEmit",
    "test:bench": "vitest bench",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.1.1",
    "@tauri-apps/api": "^1.5.3",
    "@types/better-sqlite3": "^7.6.8",
    "@types/howler": "^2.2.11",
    "@vueuse/core": "^10.7.1",
    "better-sqlite3": "^9.2.2",
    "chessground": "^9.0.4",
    "chessops": "^0.12.7",
    "date-fns": "^2.30.0",
    "howler": "^2.2.4",
    "kysely": "^0.26.3",
    "pinia": "^2.1.7",
    "radix-vue": "^1.2.5",
    "tauri-plugin-sql-api": "github:tauri-apps/tauri-plugin-sql#v1",
    "uci-parser-ts": "^0.1.4",
    "vue": "^3.4.3",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.1",
    "@rushstack/eslint-patch": "^1.6.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tauri-apps/cli": "^1.5.9",
    "@types/node": "^20.10.6",
    "@vitejs/plugin-vue": "^4.6.2",
    "@volar/vue-typescript": "^1.6.5",
    "@vue/devtools": "^6.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/typescript": "^1.8.20",
    "autoprefixer": "^10.4.16",
    "cpu-features": "^0.0.9",
    "dotenv": "^16.3.1",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.19.2",
    "execa": "^8.0.1",
    "follow-redirects": "^1.15.4",
    "happy-dom": "^12.10.3",
    "kysely-codegen": "^0.11.0",
    "node-gyp": "^10.0.1",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.10",
    "tailwindcss": "^3.4.0",
    "tar": "^6.2.0",
    "typescript": "^5.3.3",
    "unplugin-vue-router": "^0.7.0",
    "unzipper": "^0.10.14",
    "vite": "^5.0.10",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  }
}
