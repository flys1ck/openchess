{
  "name": "openchess",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devtools": "vue-devtools",
    "build": "vue-tsc --noEmit && vite build",
    "tauri": "tauri",
    "db:gen:types": "kysely-codegen --dialect sqlite --exclude-pattern __* --out-file src/database.d.ts",
    "format": "prettier --ignore-path .prettierignore --ignore-path .gitignore --write .",
    "lint": "eslint . --ext .vue,.js,.cjs,.ts --fix --ignore-path .prettierignore",
    "lint:types": "vue-tsc --noEmit",
    "test:bench": "vitest bench",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.4",
    "@headlessui/vue": "^1.7.17",
    "@heroicons/vue": "^2.1.1",
    "@tauri-apps/api": "^1.5.3",
    "@types/better-sqlite3": "^7.6.8",
    "@types/howler": "^2.2.11",
    "@vueuse/core": "^10.7.2",
    "better-sqlite3": "^9.3.0",
    "chessground": "^9.0.4",
    "chessops": "^0.13.0",
    "date-fns": "^3.3.0",
    "howler": "^2.2.4",
    "kysely": "^0.27.2",
    "pinia": "^2.1.7",
    "radix-vue": "^1.3.2",
    "tauri-plugin-sql-api": "github:tauri-apps/tauri-plugin-sql#v1",
    "uci-parser-ts": "^0.1.4",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.41.1",
    "@rushstack/eslint-patch": "^1.7.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tauri-apps/cli": "^1.5.9",
    "@types/node": "^20.11.5",
    "@vitejs/plugin-vue": "^5.0.3",
    "@vitest/coverage-v8": "^1.2.1",
    "@volar/vue-typescript": "^1.6.5",
    "@vue/devtools": "^7.0.11",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/typescript": "^1.8.20",
    "autoprefixer": "^10.4.17",
    "cpu-features": "^0.0.9",
    "dotenv": "^16.3.2",
    "eslint": "^8.56.0",
    "eslint-plugin-vue": "^9.20.1",
    "execa": "^8.0.1",
    "follow-redirects": "^1.15.5",
    "happy-dom": "^13.2.0",
    "kysely-codegen": "^0.11.0",
    "node-gyp": "^10.0.1",
    "postcss": "^8.4.33",
    "prettier": "^3.2.4",
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.1",
    "tar": "^6.2.0",
    "typescript": "^5.3.3",
    "unplugin-vue-router": "^0.7.0",
    "unzipper": "^0.10.14",
    "vite": "^5.0.12",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1",
    "vue-tsc": "^1.8.27"
  }
}
