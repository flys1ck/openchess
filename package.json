{
  "name": "openchess",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "devtools": "vue-devtools",
    "build": "vue-tsc --noEmit && vite build",
    "tauri": "tauri",
    "lint": "eslint . --ext .vue,.js,.cjs,.ts --fix --ignore-path .prettierignore",
    "format": "prettier --ignore-path .prettierignore --ignore-path .gitignore --write .",
    "db:gen:types": "kysely-codegen --dialect sqlite --exclude-pattern __* --out-file src/database.d.ts",
    "test:bench": "vitest bench",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@headlessui/vue": "^1.7.16",
    "@heroicons/vue": "^2.0.18",
    "@mliebelt/pgn-parser": "^1.4.12",
    "@tauri-apps/api": "^1.5.1",
    "@types/better-sqlite3": "^7.6.6",
    "@types/howler": "^2.2.10",
    "@vueuse/core": "^10.5.0",
    "better-sqlite3": "^9.0.0",
    "chess.js": "1.0.0-beta.6",
    "chessground": "^9.0.4",
    "chessops": "^0.12.7",
    "cm-pgn": "^3.3.0",
    "date-fns": "^2.30.0",
    "howler": "^2.2.4",
    "kysely": "^0.26.3",
    "pinia": "^2.1.7",
    "radix-vue": "^1.1.2",
    "tailwind-scrollbar": "^3.0.5",
    "tauri-plugin-sql-api": "github:tauri-apps/tauri-plugin-sql#v1",
    "uci-parser-ts": "^0.1.4",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.39.0",
    "@rushstack/eslint-patch": "^1.5.1",
    "@tailwindcss/forms": "^0.5.6",
    "@tauri-apps/cli": "^1.5.4",
    "@types/node": "^20.8.7",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vue/devtools": "^6.5.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.16",
    "dotenv": "^16.3.1",
    "eslint": "^8.51.0",
    "eslint-plugin-vue": "^9.17.0",
    "execa": "^8.0.1",
    "happy-dom": "^12.9.1",
    "kysely-codegen": "^0.11.0",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "prettier-plugin-organize-imports": "^3.2.3",
    "prettier-plugin-tailwindcss": "^0.5.6",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "unplugin-vue-router": "^0.7.0",
    "vite": "^4.5.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.19"
  }
}
